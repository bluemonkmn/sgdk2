   {
      throw new NotImplementedException("Attempted to execute rules on plan " + this.GetType().Name + " without any rules");
   }

   /// &lt;summary&gt;
   /// Allows customization of the way &lt;see cref="ExecuteRulesInternal"/&gt; is called.
